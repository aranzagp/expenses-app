<% provide(:title, 'All Incomes') %>
<h1>All incomes</h1>

<table class="table table-hover">
  <tr>
    <th>Amount</th>
    <th>Date</th> 
    <th>Category</th>
  </tr>
  <% @incomes.each do |income| %>
  <tr>
    <td><%= link_to income.amount, income %> </td>
    <td><%= income.date %> </td>
    <td><%= income.category %> </td>
  </tr>
  <% end %>
</table>